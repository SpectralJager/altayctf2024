{
	"name":"radiofan/inf-train",
	"description":"Self-written PHP service for AltayCTF2024",
	"type":"project",
	"license":"proprietary",
	"minimum-stability":"stable",
	"authors":[
		{
			"name":"RADIOFAN",
			"homepage":"https://github.com/radiofan/"
		}
	],
	"config":{
		"vendor-dir":"www/includes/vendor",
		"optimize-autoloader":true,
		"classmap-authoritative":true
	},
	"require":{
		"php":"7.4.*",
		"ext-mbstring":"*",
		"ext-sqlite3":"*",
		"ext-json":"*",
		"ext-gd":"*"
	},
	"autoload":{
		"psr-4":{
			"Core\\":"www/includes/classes/Core"
		}
	}
}